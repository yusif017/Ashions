
@model List<ContactListDTO>
<li class="nav-item dropdown border-left">
    <a class="nav-link count-indicator dropdown-toggle" id="messageDropdown" href="#" data-bs-toggle="dropdown" aria-expanded="false">
        <i class="mdi mdi-email"></i>
        <span class="count bg-success"></span>
    </a>
    <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="messageDropdown">
        <h6 class="p-3 mb-0">Messages</h6>
          @foreach (var item in Model.Take(5))
        {
        <div class="dropdown-divider"></div>
            <a href="/admin/Dashboard/Deteil/@item.Id" class="dropdown-item preview-item">
            <div class="preview-thumbnail">
                <img src="~/Admin/assets/images/faces/face4.jpg" alt="image" class="rounded-circle profile-pic">
            </div>
            <div class="preview-item-content">
                    <p href="/admin/Dashboard/Deteil/@item.Id"><strong>@item.Name</strong> @item.Content</p>
                    <p class="text-muted mb-0"> @item.PublishData.ToString("hh:mm")</p>
            </div>
        </a>
        <div class="dropdown-divider"></div>
        }
        <p class="p-3 mb-0 text-center">New messages</p>
    </div>
</li>